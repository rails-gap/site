<table id="members-table" class="table table-striped table-bordered" width="100%">
  <thead>
    <tr>
      <th>Name</th>
      <th>Actions</th>
    </tr>
  </thead>
  <% memberships.each do |membership| %>
    <tr>
      <td><%= membership.user.full_name %></td>
      <td>
        <%= link_to admin_practice_membership_delete_path(practice, membership),
          remote: true, class: 'btn btn-danger btn-sm' do %>
          <span class="glyphicon glyphicon-trash"></span>
        <% end %>
      </td>
    </tr>
  <% end %>
</table>

<script type="text/javascript">
  $(document).ready(function() {
     $('#members-table').DataTable({
       retrieve: true,
       iDisplayLength: 50,
       columns: [
         { type: "string" },
         { orderable: false }
       ]
    });
  });
</script>
