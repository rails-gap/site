$("ul.errors").html("")
<% if practice.errors.any? %>
  <% practice.errors.full_messages.each do |message| %>
    $("div.errors").append($("<div class='alert alert-danger'/>").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $("#practices-content").html("<%= escape_javascript(render 'index', practices: practices ) %>");
  $("#practices-modal").modal("hide");
<% end %>
