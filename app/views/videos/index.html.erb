<h1>Videos</h1>

<%= form_tag search_videos_path, remote: true do %>
  <div class="input-group">
    <%= autocomplete_field_tag :search, params[:search], autocomplete_video_name_videos_path,
      { class: 'search-query form-control', placeholder: 'Search', type: 'search' } %>
    <span class="input-group-btn">
      <%= submit_tag 'Go', class: 'btn btn-default' %>
    </span>
  </div>
<% end %>
<br>

<div id="videos-content">
  <%= render 'index', videos: @videos %>
</div>
